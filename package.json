{
  "name": "@raypx/i18n",
  "version": "0.0.0",
  "description": "A Vite plugin for internationalization with compile-time optimization",
  "keywords": [
    "vite",
    "vite-plugin",
    "i18n",
    "internationalization",
    "localization"
  ],
  "license": "Apache-2.0",
  "author": "",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./runtime": {
      "import": {
        "types": "./dist/runtime/index.d.mts",
        "default": "./dist/runtime/index.mjs"
      },
      "require": {
        "types": "./dist/runtime/index.d.cts",
        "default": "./dist/runtime/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "changeset": "changeset",
    "dev": "tsdown --watch",
    "lint": "eslint src --ext .ts",
    "playground": "pnpm -C playground dev",
    "pre:enter": "changeset pre enter",
    "pre:exit": "changeset pre exit",
    "prepublishOnly": "pnpm build",
    "prerelease": "pnpm build && changeset publish",
    "release": "pnpm build && changeset publish && changeset tag && git push --follow-tags",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "version": "changeset version"
  },
  "dependencies": {
    "magic-string": "^0.30.21"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@types/node": "^24.10.1",
    "eslint": "^8.56.0",
    "tsdown": "^0.16.5",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.10"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0"
  },
  "packageManager": "pnpm@10.22.0"
}
